{
  "extends": "@deephaven/tsconfig",
  "compilerOptions": {
    "emitDeclarationOnly": false,
    "noEmit": true
  },
  "include": [
    "plugins/**/**.ts",
    "plugins/**/**.tsx",
    "plugins/**/**.js",
    "plugins/**/**.jsx",
    "tests/**/**.ts"
  ],
  "exclude": ["**/node_modules", "plugins/*/src/js/dist/**/*"],
  "watchOptions": {
    "excludeDirectories": ["**/node_modules", "**/dist"]
  }
}
